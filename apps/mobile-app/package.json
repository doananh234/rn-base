{
  "name": "book",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "init-ios": "./node_modules/.bin/generate-icon ./customIcon/styles.css --componentName=mealplanner  --prefix=icon- --fontFamily=mealplanner > ./node_modules/react-native-vector-icons/mealplanner.js && cp ./customIcon/fonts/mealplanner.ttf ./node_modules/react-native-vector-icons/Fonts/mealplanner.ttf && cd ios && pod install && cd ..",
    "createIconFont": "./node_modules/.bin/generate-icon ./customIcon/styles.css --componentName=mealplanner  --prefix=icon- --fontFamily=mealplanner > ./node_modules/react-native-vector-icons/mealplanner.js && cp ./customIcon/fonts/mealplanner.ttf ./node_modules/react-native-vector-icons/Fonts/mealplanner.ttf",
    "buildandroid": "cd android && ./gradlew clean && ./gradlew assembleRelease",
    "postinstall": "yarn run createIconFont",
    "codePush": "code-push release-react app-ios ios && code-push release-react app-android android",
    "codePushProduction": "code-push promote app-ios Staging Production && code-push promote app-android Staging Production"
  },
